  "author": {
    "name": "Google, LLC"
  },
  "license": "MIT",
  "dependencies": {
    "gaxios": "^6.7.1",
    "jws": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@types/jws": "^3.2.10",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.9.0",
    "c8": "^10.1.2",
    "cheerio": "^1.0.0",
    "gts": "^6.0.2",
    "jsdoc": "^4.0.4",
    "jsdoc-fresh": "^3.0.0",
    "jsdoc-region-tag": "^3.0.0",
    "linkinator": "^6.1.2",
    "mocha": "^10.8.2",
    "nock": "^13.5.6",
    "pdfmake": "^0.2.15",
    "typescript": "^5.6.3"
